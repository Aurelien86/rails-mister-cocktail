<h1>Cocktails list</h1>
<button><%= link_to 'Create new cocktail', new_cocktail_path%></button>

    <div class="row align-item center">
    <% @cocktails.each do |cocktail|%>
        <div class="card d-flex">
        <li><%= cocktail.name %> <%= link_to 'find out', cocktail_path(cocktail.id)%></li>
    <% end %>
  </div>

<div class="container" style="row align-items-start">
